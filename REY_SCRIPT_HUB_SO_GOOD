-- Section 3: Custom Text
local Section3 = createSection("üìù Custom Text System", 3)
Section3.Size = UDim2.new(1, 0, 0, 90)

local CustomTextBox = createTextbox(Section3, "Enter Account ID", UDim2.new(0, 10, 0, 35))

local CustomTextButton = Instance.new("TextButton")
CustomTextButton.Size = UDim2.new(0, 100, 0, 35)
CustomTextButton.Position = UDim2.new(0, 270, 0, 35)
CustomTextButton.BackgroundColor3 = Color3.fromRGB(65, 150, 220)
CustomTextButton.Text = "Execute"
CustomTextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
CustomTextButton.TextSize = 14
CustomTextButton.Font = Enum.Font.GothamBold
CustomTextButton.BorderSizePixel = 0
CustomTextButton.Parent = Section3

local CustomTextButtonCorner = Instance.new("UICorner")
CustomTextButtonCorner.CornerRadius = UDim.new(0, 6)
CustomTextButtonCorner.Parent = CustomTextButton

CustomTextButton.MouseButton1Click:Connect(function()
    if CustomTextBox.Text ~= "" then
        pcall(function()
            local args = {CustomTextBox.Text}
            game:GetService("ReplicatedStorage"):WaitForChild("CustomTextSystem"):WaitForChild("RequestCustomText"):InvokeServer(unpack(args))
        end)
    end
end)

CustomTextButton.MouseEnter:Connect(function()
    TweenService:Create(CustomTextButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(85, 170, 240)}):Play()
end)

CustomTextButton.MouseLeave:Connect(function()
    TweenService:Create(CustomTextButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(65, 150, 220)}):Play()
end)

