    DupeSliderButton.Text = dupeFishSpeed .. "x"
end)

-- Dupe Button Click Event
DupeButton.MouseButton1Click:Connect(function()
    dupeFishLoop = not dupeFishLoop
    
    if dupeFishLoop then
        DupeButton.Text = "Stop Loop"
        DupeButton.BackgroundColor3 = Color3.fromRGB(220, 80, 80)
        
        -- Start the loop
        task.spawn(function()
            while dupeFishLoop do
                pcall(function()
                    -- Script 3
                    local args = {
                        {duration = 7.5632384619966615, result = "SUCCESS", insideRatio = 0.8},
                        "35d75c78-906f-461b-ad9e-866ce933ad3c"
                    }
                    game:GetService("ReplicatedStorage"):WaitForChild("Fishing"):WaitForChild("ToServer"):WaitForChild("ReelFinished"):FireServer(unpack(args))
                end)
                
                task.wait(1 / dupeFishSpeed)
            end
        end)
    else
        DupeButton.Text = "Start Loop"
        DupeButton.BackgroundColor3 = Color3.fromRGB(65, 150, 220)
    end
end)

