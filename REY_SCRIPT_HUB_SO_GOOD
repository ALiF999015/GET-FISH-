ContentFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y
ContentFrame.Parent = MainFrame

local ContentLayout = Instance.new("UIListLayout")
ContentLayout.Padding = UDim.new(0, 10)
ContentLayout.SortOrder = Enum.SortOrder.LayoutOrder
ContentLayout.Parent = ContentFrame

-- Function to create section
local function createSection(title, order)
    local Section = Instance.new("Frame")
    Section.Name = title
    Section.Size = UDim2.new(1, 0, 0, 80)
    Section.BackgroundColor3 = Color3.fromRGB(200, 230, 255)
    Section.BorderSizePixel = 0
    Section.LayoutOrder = order
    Section.Parent = ContentFrame
    
    local SectionCorner = Instance.new("UICorner")
    SectionCorner.CornerRadius = UDim.new(0, 8)
    SectionCorner.Parent = Section
    
    local SectionTitle = Instance.new("TextLabel")
    SectionTitle.Size = UDim2.new(1, -20, 0, 25)
    SectionTitle.Position = UDim2.new(0, 10, 0, 5)
    SectionTitle.BackgroundTransparency = 1
    SectionTitle.Text = title
    SectionTitle.TextColor3 = Color3.fromRGB(30, 30, 30)
    SectionTitle.TextSize = 16
    SectionTitle.Font = Enum.Font.GothamBold
    SectionTitle.TextXAlignment = Enum.TextXAlignment.Left
    SectionTitle.Parent = Section
    
    return Section
end

-- Function to create textbox
local function createTextbox(parent, placeholder, position)
    local Textbox = Instance.new("TextBox")
    Textbox.Size = UDim2.new(0, 250, 0, 35)
    Textbox.Position = position
    Textbox.BackgroundColor3 = Color3.fromRGB(220, 240, 255)
    Textbox.PlaceholderText = placeholder
    Textbox.Text = ""
    Textbox.TextColor3 = Color3.fromRGB(30, 30, 30)
    Textbox.PlaceholderColor3 = Color3.fromRGB(100, 100, 100)
    Textbox.TextSize = 14
    Textbox.Font = Enum.Font.Gotham
    Textbox.BorderSizePixel = 0
    Textbox.ClearTextOnFocus = false
    Textbox.Parent = parent
    
    local TextboxCorner = Instance.new("UICorner")
    TextboxCorner.CornerRadius = UDim.new(0, 6)
    TextboxCorner.Parent = Textbox
    
    return Textbox
end

-- Section 1: Auto Fishing
local Section1 = createSection("üé£ Auto Fishing", 1)
Section1.Size = UDim2.new(1, 0, 0, 90)

local FishingButton = Instance.new("TextButton")
FishingButton.Name = "FishingButton"
FishingButton.Size = UDim2.new(0, 120, 0, 35)
FishingButton.Position = UDim2.new(0, 10, 0, 35)
FishingButton.BackgroundColor3 = Color3.fromRGB(65, 150, 220)
FishingButton.Text = "Start Loop"
FishingButton.TextColor3 = Color3.fromRGB(255, 255, 255)
FishingButton.TextSize = 14
FishingButton.Font = Enum.Font.GothamBold
FishingButton.BorderSizePixel = 0
FishingButton.Parent = Section1

local FishingButtonCorner = Instance.new("UICorner")
FishingButtonCorner.CornerRadius = UDim.new(0, 6)
FishingButtonCorner.Parent = FishingButton

local SpeedLabel = Instance.new("TextLabel")
SpeedLabel.Size = UDim2.new(0, 60, 0, 25)
SpeedLabel.Position = UDim2.new(0, 140, 0, 40)
SpeedLabel.BackgroundTransparency = 1
SpeedLabel.Text = "Speed:"
SpeedLabel.TextColor3 = Color3.fromRGB(30, 30, 30)
SpeedLabel.TextSize = 14
SpeedLabel.Font = Enum.Font.GothamBold
SpeedLabel.Parent = Section1

-- Speed Slider for Fishing
local FishingSliderFrame = Instance.new("Frame")
FishingSliderFrame.Size = UDim2.new(0, 100, 0, 25)
FishingSliderFrame.Position = UDim2.new(0, 200, 0, 40)
FishingSliderFrame.BackgroundColor3 = Color3.fromRGB(150, 200, 230)
FishingSliderFrame.BorderSizePixel = 0
FishingSliderFrame.Parent = Section1

local FishingSliderCorner = Instance.new("UICorner")
FishingSliderCorner.CornerRadius = UDim.new(0, 4)
FishingSliderCorner.Parent = FishingSliderFrame

local FishingSliderButton = Instance.new("TextButton")
FishingSliderButton.Size = UDim2.new(0.5, 0, 1, 0)
FishingSliderButton.Position = UDim2.new(0, 0, 0, 0)
FishingSliderButton.BackgroundColor3 = Color3.fromRGB(65, 150, 220)
FishingSliderButton.Text = "1x"
FishingSliderButton.TextColor3 = Color3.fromRGB(255, 255, 255)
FishingSliderButton.TextSize = 12
FishingSliderButton.Font = Enum.Font.GothamBold
FishingSliderButton.BorderSizePixel = 0
FishingSliderButton.Parent = FishingSliderFrame

local FishingSliderButtonCorner = Instance.new("UICorner")
FishingSliderButtonCorner.CornerRadius = UDim.new(0, 4)
FishingSliderButtonCorner.Parent = FishingSliderButton

-- Fishing Speed Control
FishingSliderButton.MouseButton1Click:Connect(function()
    fishingSpeed = fishingSpeed + 1
    if fishingSpeed > 5 then fishingSpeed = 1 end
    FishingSliderButton.Text = fishingSpeed .. "x"
end)

-- Fishing Button Click Event
FishingButton.MouseButton1Click:Connect(function()
    fishingLoop = not fishingLoop
    
    if fishingLoop then
        FishingButton.Text = "Stop Loop"
        FishingButton.BackgroundColor3 = Color3.fromRGB(220, 80, 80)
        
        -- Start the loop
        task.spawn(function()
            while fishingLoop do
                pcall(function()
                    -- Script 1
                    local args1 = {1, "35d75c78-906f-461b-ad9e-866ce933ad3c"}
                    game:GetService("ReplicatedStorage"):WaitForChild("Fishing_RemoteThrow"):FireServer(unpack(args1))
                    
                    task.wait(0.5)
                    
                    -- Script 2
                    local args2 = {
                        {duration = 7.5632384619966615, result = "SUCCESS", insideRatio = 0.8},
                        "35d75c78-906f-461b-ad9e-866ce933ad3c"
                    }
                    game:GetService("ReplicatedStorage"):WaitForChild("Fishing"):WaitForChild("ToServer"):WaitForChild("ReelFinished"):FireServer(unpack(args2))
                end)
                
                task.wait(1 / fishingSpeed)
            end
        end)
    else
        FishingButton.Text = "Start Loop"
        FishingButton.BackgroundColor3 = Color3.fromRGB(65, 150, 220)
    end
end)

-- Hover effects
FishingButton.MouseEnter:Connect(function()
    if not fishingLoop then
        TweenService:Create(FishingButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(85, 170, 240)}):Play()
    end
end)

FishingButton.MouseLeave:Connect(function()
    if not fishingLoop then
        TweenService:Create(FishingButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(65, 150, 220)}):Play()
    end
end)

